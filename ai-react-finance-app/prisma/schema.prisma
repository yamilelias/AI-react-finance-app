generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Expense {
  id          Int      @id @default(autoincrement())
  description String
  amount      Float
  category    String
  date        DateTime
  createdAt   DateTime @default(now())
  userId      String
}

model Account {
  id        Int      @id @default(autoincrement())
  userId    String   
  email     String   @unique
  createdAt DateTime @default(now())
}